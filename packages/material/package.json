{
  "name": "@chamn/material",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "version": "0.10.3",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module-sync": "./dist/index.es.js",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./dist/meta": {
      "types": "./dist/meta.d.ts",
      "module-sync": "./dist/meta.es.js",
      "import": "./dist/meta.es.js",
      "require": "./dist/meta.cjs.js"
    },
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "start": "cross-env DEV=true build-script",
    "dev": "cross-env DEV=true build-script",
    "build": "pnpm run build:lib && pnpm run build:meta",
    "build:lib": "cross-env LIB_NAME=comp  build-script --build",
    "build:meta": "cross-env LIB_NAME=meta build-script --build",
    "lint": "eslint ./src",
    "prettier": "prettier --write ./src",
    "test": "jest"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "dependencies": {
    "antd": "^5.23.2",
    "axios": "^1.8.4",
    "clsx": "^2.1.1",
    "gridstack": "^11.1.1",
    "loadjs": "^4.3.0",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^6.2.2"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.0.1",
    "@chamn/build-script": "workspace:*",
    "@chamn/engine": "workspace:*",
    "@chamn/model": "workspace:*",
    "@chamn/render": "workspace:*",
    "@monaco-editor/react": "^4.5.1",
    "@types/loadjs": "^4.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "~18.15.9",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "cross-env": "^7.0.3",
    "react-router-dom": "^6.10.0",
    "sass": "^1.62.0",
    "vite-plugin-external": "^6.0.1"
  },
  "config": {},
  "gitHead": "dc3e55fdeb903a8012f6ebd3ebc018ed61ad89db"
}
